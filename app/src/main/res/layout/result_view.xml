<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.soronto.mca.greedmeasure.MainActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">
    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">
        <TextView
            android:id="@+id/title"
            android:layout_width="325dp"
            android:layout_height="65dp"
            android:text="RESULT"
            android:textColor="#343434"
            android:textSize="25sp"
            tools:layout_editor_absoluteY="37dp"
            tools:layout_editor_absoluteX="27dp" />
        <TextView
            android:id="@+id/result"
            android:layout_width="325dp"
            android:layout_height="65dp"
            android:text="RES234T"
            android:textColor="#343434"
            android:textSize="25sp"
            tools:layout_editor_absoluteY="102dp"
            tools:layout_editor_absoluteX="30dp" />

        <LinearLayout
            android:id="@+id/graph"
            android:layout_width="358dp"
            android:layout_height="61dp"
            android:background="#ffc3c3"
            android:orientation="horizontal"
            android:visibility="visible"
            tools:ignore="InvalidId"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="167dp"></LinearLayout>

        <LinearLayout
            android:layout_width="358dp"
            android:layout_height="8dp"
            android:background="#234d6e"
            android:orientation="horizontal"
            android:visibility="visible"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="228dp"></LinearLayout>
    </LinearLayout>



</android.support.constraint.ConstraintLayout>
